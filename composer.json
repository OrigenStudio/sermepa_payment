{
  "name": "drupal/sermepa_payment",
  "description": "Provides Sermepa compatibility for Payment module.",
  "type": "drupal-module",
  "require": {
    "drupal/payment": "8.2.0-rc3",
    "drupal/payment_offsite_api": "8.2.0-rc1",
    "commerce_redsys/sermepa": "*"
  },
  "repositories": [
      {
          "type": "git",
          "url": "https://github.com/simplelogica/sermepa_php"
      }
  ],
  "autoload": {
    "psr-4": {
        "CommerceRedsys\\Payment\\": "Sermepa/src/"
    }
  },
  "minimum-stability": "dev"
}
